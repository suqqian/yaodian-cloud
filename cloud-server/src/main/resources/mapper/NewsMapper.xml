<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.template.edurptemplate.news.mapper.NewsMapper">

    <resultMap type="com.template.edurptemplate.news.entity.NewsDTO" id="NewsMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="addtime" column="addtime" jdbcType="TIMESTAMP"/>
        <result property="title" column="title" jdbcType="VARCHAR"/>
        <result property="introduction" column="introduction" jdbcType="OTHER"/>
        <result property="picture" column="picture" jdbcType="VARCHAR"/>
        <result property="content" column="content" jdbcType="OTHER"/>
    </resultMap>

    <!--查询单个-->
    <sql id="Base_Column_List">
        id
        , addtime, title, introduction, picture, content    </sql>


</mapper>
